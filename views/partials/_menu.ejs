<!-- header -->
<header class="header-area">
    <div class="container-fluid">
       <div class="row ">
         
          <div class=" col-md-2 col-sm-3">
             <div class="logo">
                <a href="/">Photo<br><span>Pedia</span></a>
             </div>
          </div>
          <div class="col-md-10">
             <div class="navbar-area" style="float: right;">
                <nav class="site-navbar">
                   <ul>
                      <li><a class="<%= activePage  === 'home' ? 'active' : '' %>" href="/">Home</a></li>
                      <li><a class="<%= activePage  === 'about' ? 'active' : '' %>" href="/about">About</a></li>
                      <li><a class="<%= activePage  === 'photos' ? 'active' : '' %>" href="/photos">Photos</a></li>
                      
                      <% if (user) { %>
                        <li><a class="<%= activePage  === 'dashboard' ? 'active' : '' %>" href="/users/dashboard">Dashboard</a></li>
                        <li><a  href="/logout">Logout</a></li>
                        <% } else {%>
                           <li><a class="<%= activePage  === 'register' ? 'active' : '' %>" href="/register">Register</a></li>
                           <li><a class="<%= activePage  === 'login' ? 'active' : '' %>" href="/login">Login</a></li>

                      <% } %>
                   </ul>
                   <button class="nav-toggler">
                   <span></span>
                   </button>
                </nav>
             </div>
          </div>
